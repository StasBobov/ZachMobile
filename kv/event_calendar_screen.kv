#:import event_calendar event_calendar

<EventCalendarScreen>:
    BoxLayout:
        orientation: "vertical"
        size_hint: 1, 1
        BoxLayout:
            orientation: "horizontal"
            size_hint: 1, .15
            ImageButton:
                size_hint: .15, 1
                pos_hint: {"top": 1, "right": .25}
                source: "icons/back.png"
                on_release:
                    app.change_screen(app.previous_screen)
                    app.previous_screen = "event_calendar_screen"
            ImageButton:
                size_hint: .25, 1
                source: "icons/cal_back.png"
                on_release:
                    event_calendar.month_back()
            Label:
                id: month
                text: ""
                text_size: self.size
                halign: 'center'
                valign: 'middle'
                font_size: '20sp'
                pos_hint: {"top": 1, "right": 1}
                canvas.before:
                    Color:
                        rgb: utils.get_color_from_hex('#000080')
                    Rectangle:
                        size: self.size
                        pos: self.pos
            ImageButton:
                size_hint: .25, 1
                source: "icons/cal_next.png"
                on_release:
                    event_calendar.month_next()

        GridLayout:
            id: calendar_layout
            name: "calendar_layout"
            cols: 7
            rows: 7
            ### row 1 ###
            Button:
                text: 'Mon'
            Button:
                text: 'Tue'
            Button:
                text: 'Wed'
            Button:
                text: 'Thu'
            Button:
                text: 'Fri'
            Button:
                text: 'Sat'
            Button:
                text: 'Sun'

            ### row 2 ###
            BoxLayout:
                orientation: 'vertical'
                Label:
                    id: 0l
                    text: "No ev"
                    font_size: '13sp'
                    size_hint: 1, .25
                    pos_hint: {"top": 1, "right": 1}
                    background_color: (24/255, 171/255, 21/255, 1)
                    canvas.before:
                        Color:
                            rgba: self.background_color
                        Rectangle:
                            size: self.size
